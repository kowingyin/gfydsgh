<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fyp.kwl.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".search.SearchMenu" />
        <activity
            android:name=".search.GeneralSearch"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".detail.Detail" />
        <activity android:name=".Nevigate" />
        <activity android:name=".Favorite"></activity>
    </application>

</manifest>